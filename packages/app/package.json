{
  "name": "@galacean/devtools-app",
  "type": "module",
  "version": "28.0.0",
  "private": true,
  "description": "Galacean DevTools",
  "author": "YunYouJun <me@yunyoujun.cn>",
  "license": "MIT",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue"
  ],
  "main": "dist-electron/main/index.js",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:app": "vite",
    "dev:lib": "tsx scripts/build.ts --watch",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build:lib": "vue-tsc --noEmit && vite build && tsx scripts/build.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "colord": "^2.9.3",
    "h3": "^1.9.0",
    "ip": "^1.1.8",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.27",
    "@iconify-json/ri": "^1.1.18",
    "@types/ip": "^1.1.3",
    "@vitejs/plugin-vue": "^5.0.1",
    "@vueuse/core": "^10.1.2",
    "consola": "^3.2.3",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "shikiji": "^0.9.17",
    "tree-kill": "^1.2.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.0-beta.2",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.1",
    "vue-tsc": "^1.8.27"
  }
}
