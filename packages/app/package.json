{
  "name": "@galacean/devtools-app",
  "type": "module",
  "version": "28.0.0",
  "private": true,
  "description": "Galacean DevTools",
  "author": "YunYouJun <me@yunyoujun.cn>",
  "license": "MIT",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue"
  ],
  "main": "dist-electron/main/index.js",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:app": "vite",
    "dev:lib": "tsx scripts/build.ts --watch",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build:lib": "vue-tsc --noEmit && vite build && tsx scripts/build.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "colord": "^2.9.3",
    "h3": "^1.11.1",
    "ip": "^1.1.9",
    "pinia": "^2.1.7",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "splitpanes": "^3.1.5"
  },
  "devDependencies": {
    "@advjs/gui": "0.0.3-beta.10",
    "@galacean/devtools-core": "workspace:*",
    "@galacean/engine": "1.1.0-beta.32",
    "@iconify-json/carbon": "^1.1.31",
    "@iconify-json/ri": "^1.1.20",
    "@types/ip": "^1.1.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vueuse/core": "^10.9.0",
    "consola": "^3.2.3",
    "electron": "^28.3.1",
    "electron-builder": "^24.13.3",
    "sass": "^1.75.0",
    "shikiji": "^0.10.2",
    "tree-kill": "^1.2.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.2.9",
    "vite-plugin-electron": "^0.28.5",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.23",
    "vue-tsc": "^1.8.27"
  }
}
